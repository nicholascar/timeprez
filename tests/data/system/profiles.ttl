PREFIX altp: <http://www.w3.org/ns/dx/conneg/altr>
PREFIX altr: <http://www.w3.org/ns/dx/conneg/altr#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX prof: <http://www.w3.org/ns/dx/prof/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

#
#   Classes
#
rdf:Resource
    altr:hasDefaultRepresentation _:altp-html ;
    altr:hasRepresentation
        _:altp-turtle ,
        _:altp-xml ,
        _:altp-jsonld ,
        _:altp-n3 ,
        _:altp-nt ,
        _:altp-html ;
.

#
#   Alt
#
altp:
    a prof:Profile ;
    rdfs:label "Alternate Representations" ;
    rdfs:comment "The representation of the resource that lists all other representations (profiles and Media Types)" ;
    dcterms:identifier "alt"^^xsd:token ;
.

#
#   Media Types
#
<https://w3id.org/mediatype/text/turtle>
    a dcterms:MediaType ;
    rdfs:label "Turtle" ;
    dcterms:identifier "text/turtle"^^xsd:token ;
.

<https://w3id.org/mediatype/application/rdf+xml>
    a dcterms:MediaType ;
    rdfs:label "RDF/XML" ;
    dcterms:identifier "application/rdf+xml"^^xsd:token ;
.

<https://w3id.org/mediatype/application/ld+json>
    a dcterms:MediaType ;
    rdfs:label "JSON-LD" ;
    dcterms:identifier "application/ld+json"^^xsd:token ;
.

<https://w3id.org/mediatype/text/n3>
    a dcterms:MediaType ;
    rdfs:label "N3" ;
    dcterms:identifier "text/n3"^^xsd:token ;
.

<https://w3id.org/mediatype/application/n-triples>
    a dcterms:MediaType ;
    rdfs:label "N-Triples" ;
    dcterms:identifier "application/n-triples"^^xsd:token ;
.

<https://w3id.org/mediatype/application/json>
    a dcterms:MediaType ;
    rdfs:label "JSON" ;
    dcterms:identifier "application/json"^^xsd:token ;
.

<https://w3id.org/mediatype/text/html>
    a dcterms:MediaType ;
    rdfs:label "HTML" ;
    dcterms:identifier "text/html"^^xsd:token ;
.

<https://w3id.org/mediatype/application/geo+json>
    a dcterms:MediaType ;
    rdfs:label "GeoJSON" ;
    dcterms:identifier "application/geo+json"^^xsd:token ;
.

#
#   Representations
#
#   Alt
_:altp-turtle
    a altr:Representation ;
    dcterms:conformsTo altp: ;
    dcterms:format <https://w3id.org/mediatype/text/turtle> ;
.

_:altp-xml
    a altr:Representation ;
    dcterms:conformsTo altp: ;
    dcterms:format <https://w3id.org/mediatype/application/rdf+xml> ;
.

_:altp-jsonld
    a altr:Representation ;
    dcterms:conformsTo altp: ;
    dcterms:format <https://w3id.org/mediatype/application/ld+json> ;
.

_:altp-json
    a altr:Representation ;
    dcterms:conformsTo altp: ;
    dcterms:format <https://w3id.org/mediatype/application/json> ;
.

_:altp-n3
    a altr:Representation ;
    dcterms:conformsTo altp: ;
    dcterms:format <https://w3id.org/mediatype/text/n3> ;
.

_:altp-nt
    a altr:Representation ;
    dcterms:conformsTo altp: ;
    dcterms:format <https://w3id.org/mediatype/application/n-triples> ;
.

_:altp-html
    a altr:Representation ;
    dcterms:conformsTo altp: ;
    dcterms:format <https://w3id.org/mediatype/text/html> ;
.